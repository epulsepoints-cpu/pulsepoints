{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "public": true,
  "headers": [
    {
      "source": "/lessons-data/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}